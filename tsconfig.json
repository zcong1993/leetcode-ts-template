{
  "compilerOptions": {
    "declaration": true,
    "module": "commonjs",
    "target": "es2017",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "./dist",
    "allowJs": true,
    "isolatedModules": true
  },
  "exclude": ["node_modules", "dist", ".eslintrc.js"],
  "include": ["solve"]
}
